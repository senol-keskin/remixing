    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.4",
    "@remix-run/node": "1.7.0",
    "@remix-run/react": "1.7.0",
    "@remix-run/serve": "^1.7.0",
    "dayjs": "1.11.5",
    "framer-motion": "7.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
